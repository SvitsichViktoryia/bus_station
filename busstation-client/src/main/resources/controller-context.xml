<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
        				http://www.springframework.org/schema/beans/spring-beans.xsd
        				http://www.springframework.org/schema/context 
        				http://www.springframework.org/schema/context/spring-context.xsd
        				">

	<import resource="classpath*:spring-config.xml" />

	<bean id="authCommand" class="by.bsuir.busstation.command.AuthCommand" />
	<bean id="regCommand" class="by.bsuir.busstation.command.RegistrationCommand" />
	<bean id="ticketCommand" class="by.bsuir.busstation.command.TicketsCommand" />
	<bean id="mainCommand" class="by.bsuir.busstation.command.MainCommand" />
	<bean id="logoutCommand" class="by.bsuir.busstation.command.LogoutCommand" />
	<bean id="unknownCommand" class="by.bsuir.busstation.command.UnknownCommand" />

</beans>