<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
        				http://www.springframework.org/schema/beans/spring-beans.xsd
        				http://www.springframework.org/schema/context 
        				http://www.springframework.org/schema/context/spring-context.xsd
        				">

	<import resource="classpath*:spring-config.xml" />

	<bean id="authCommand" class="by.bsuir.busstation.command.AuthCommand" />
	<bean id="mainCommand" class="by.bsuir.busstation.command.MainCommand" />
	<bean id="placesCommand" class="by.bsuir.busstation.command.PlacesCommand" />
	<bean id="driverCommand" class="by.bsuir.busstation.command.DriverCommand" />
	<bean id="busCommand" class="by.bsuir.busstation.command.BusCommand" />
	<bean id="ticketCommand" class="by.bsuir.busstation.command.TicketCommand" />
	<bean id="logoutCommand" class="by.bsuir.busstation.command.LogoutCommand" />
	<bean id="unknownCommand" class="by.bsuir.busstation.command.UnknownCommand" />
	<bean id="userCommand" class="by.bsuir.busstation.command.UserCommand" />
</beans>